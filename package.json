{
  "name": "base64-to-file-cli",
  "version": "0.0.0",
  "description": "CLI to decode Base64 strings to files",
  "keywords": [
    "base64",
    "decode",
    "pdf",
    "file",
    "cli",
    "convert"
  ],
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "package": "pkg ./dist/index.js --out-dir ./dist",
    "build": "rollup -c ./rollup.config.js",
    "postbuild": "npm run package"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": "https://github.com/dfrankland/base64-to-file-cli.git",
  "bugs": "https://github.com/dfrankland/base64-to-file-cli/issues",
  "author": "Dylan Frankland <dylan@frankland.io>",
  "homepage": "https://npmjs.com/package/base64-to-file-cli",
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "pkg": "^3.0.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "yargs": "^7.1.0"
  }
}
